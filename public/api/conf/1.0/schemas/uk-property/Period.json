{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "title": "Period",
    "properties": {
        "id": {
            "title": "ID",
            "description": "Unique identifier",
            "type": "string"
        },
        "from": {
            "title": "From date",
            "type": "string",
            "format": "date"
        },
        "to": {
            "title": "To date",
            "type": "string",
            "format": "date"
        },
        "incomes": {
            "$ref": "Incomes.json"
        },
        "expenses": {
            "$ref": "Expenses.json"
        },
        "privateUseAdjustment": {
            "title": "Private use adjustment",
            "type": "number",
            "minimum": 0
        },
        "balancingCharge": {
            "type": "number",
            "minimum": 0
        }
    },
    "required": [
        "from",
        "to"
    ],
    "additonalProperties": false
}
